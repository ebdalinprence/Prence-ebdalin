import java.util.*;
public class PRENCE1
{
    public static void main(String [] args){
        Scanner input = new Scanner(System.in);
        int num;
        
        do{
            System.out.print("Enter a number between 1-100:");
            num = input.nextInt();
            }while(num <1 ||num>100);
            System.out.print("Valid number entered:"+num);
    }
}
